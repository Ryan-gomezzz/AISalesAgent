{
  "name": "aisalesagent",
  "version": "1.0.0",
  "description": "AISalesAgent MVP - AI assistant for chartered accountancy practice",
  "scripts": {
    "setup": "bash scripts/setup.sh",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "prepare:lambda": "node scripts/prepare-lambda.js",
    "deploy:backend": "npm run build:backend && npm run prepare:lambda && npx serverless deploy --stage prod",
    "deploy": "bash scripts/deploy.sh"
  },
  "keywords": [
    "ai",
    "assistant",
    "bedrock",
    "polly",
    "transcribe",
    "rekognition"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.25.12",
    "serverless": "^3.40.0",
    "serverless-esbuild": "^1.55.2",
    "serverless-offline": "^13.9.0"
  }
}
